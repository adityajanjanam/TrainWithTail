        <label for="selectedUserPet" style="color: chocolate;"><b>Select Pet Parent Username and Petname:</b></label>
        <select name="selectedUserPet" id="selectedUserPet" required>
            <option>Select User_Pet</option>
            <% users.forEach(function(user) { %>
                <% if (user.role === 'Customer') { %>
                    <option value="<%= user.name %>_<%= user.petName %>" <% if (user.name + '_' + user.petName === selectedUserPet) { %> selected <% } %>>
                        <%= user.name %>_<%= user.petName %>
                    </option>
                <% } %>
            <% }); %>
        </select>

        <div class="behaviorContainer">
            <form id="progressFormDay1" class="behavior-behavior" action="/submitDay" method="POST" enctype="multipart/form-data">
                <h2 style="font-family: emoji;">Day1</h2>
                <input type="hidden" name="progressUser" id="progressUserDay1" value="">
                <input type="hidden" name="progressPet" id="progressPetDay1" value="">
                <div style="display: flex; align-items: center; margin: 20px; padding-left: 80px; margin-top: 20px;">
                    <label for="video-upload" style="color: black; margin: 10px;"><b>Upload Video:</b></label>
                    <% const filteredProgress1 = progresses.filter(progress => progress.progressUser === petUser && progress.progressDay === "Day1"); %>
                    <% if (filteredProgress1.length > 0) { %>
                        <% const behavior = filteredProgress1[0]; %>
                        <input type="file" id="video-upload" name="video" accept="video/*">
                    <% } else { %>
                        <input type="file" id="video-upload" name="video" accept="video/*">
                    <% } %>
                </div>
                
                <% if (filteredProgress1.length > 0) { %>
                    <% const behavior = filteredProgress1[0]; %>
                    <label style="color: black; margin-bottom: 10px;"><b>Current Video:</b> <%= behavior.progressVideo %></label>
                <% } %>
                <div class="behavior-details">
                    <div class="behavior-status">
                    </div>
                    <div style="display: flex; align-items: center; padding-left: 20px;">
                        <label for="behavior1-comments" style="color: black; margin: 10px;"><b>Comments:</b></label>
                        <% if (filteredProgress1.length > 0) { %>
                            <% const behavior = filteredProgress1[0]; %>
                            <textarea class="notes" id="behavior1-comments" name="comments" placeholder="Enter comments..."><%= behavior.comments %></textarea>
                        <% } else { %>
                            <textarea class="notes" id="behavior1-comments" name="comments" placeholder="Enter comments..."></textarea>
                        <% } %>
                    </div>
                </div>
                <input type="hidden" name="progressDay" value="Day1">
                <button type="submit" style="margin: 20px; padding: 10px 20px; background-color:#04b89afa; color: #fff; border: none; border-radius: 5px; cursor: pointer;">Save</button>
            </form>

